.UCenter-bg {
  height: 100vh;
  justify-content: center;
  overflow: hidden;
  width:100%;
  background-color:#43c0ff;
  /* text-shadow: 0 0 3px rgba(0,0,0,0.3); */
}
.BCenter-bg {
  background-image: url(https://cdn.aiipos.cn/login_orgin.png);
  background-size: 70%;
  background-repeat: no-repeat;
  background-position: center;
  height: 100vh;
  overflow: hidden;
  position: absolute;
  left:0rpx;
  top:0rpx;
  width:100%;
  text-shadow: 0 0 3px rgba(0,0,0,0.3);
  opacity: 0.7;
}
.logotitle{
  background-image: url("data:image/svg+xml;base64,");
  background-repeat: no-repeat;
  background-size: 100%;
  background-position: center;
  height:300rpx;
  background-color:white;
  width:100%;
}
.pos{
  margin-top:60vh;
}
.bg-newbutton{
  background-color:white;
  color:#010057;
  margin-top:70rpx;
  border-radius: 50vh;
  box-shadow: 4rpx 4rpx 3rpx #888888;
}
.UCenter-bg text{
  opacity: 0.8;
}
.UCenter-bg image {
  width: 250rpx;
  height: 250rpx;
}